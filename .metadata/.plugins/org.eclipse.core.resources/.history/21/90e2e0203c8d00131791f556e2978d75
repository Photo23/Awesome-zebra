import java.util.Scanner;

public class HelloPrinter
    {
       public static void main(String[] args)
       {
    	   int i = 4;
    		int j = 4;
    		i = i++;
    		System.out.println("i is " + i++);
    		System.out.println("j is " +  -j);
    		System.out.println("What is this " + i/2.0);
      }
   }

